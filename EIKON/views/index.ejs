<!DOCTYPE html>
<html lang="en">

<%- include('partials/head', { pageTitle: "Eikon | Bienvenidos" }) %>

  <body>
    <!-- HEADER-->
    <%- include('partials/header') %>
      <!-- SLIDER-->
      <div id="eikonHomeCarousel" class="carousel slide carousel-fade" data-bs-ride="carousel">
        <ol class="carousel-indicators">
          <li data-bs-target="#eikonHomeCarousel" data-bs-slide-to="0" class="active"></li>
          <li data-bs-target="#eikonHomeCarousel" data-bs-slide-to="1"></li>
          <li data-bs-target="#eikonHomeCarousel" data-bs-slide-to="2"></li>
          <li data-bs-target="#eikonHomeCarousel" data-bs-slide-to="3"></li>
          <li data-bs-target="#eikonHomeCarousel" data-bs-slide-to="4"></li>
          <li data-bs-target="#eikonHomeCarousel" data-bs-slide-to="5"></li>
          <li data-bs-target="#eikonHomeCarousel" data-bs-slide-to="6"></li>
          <li data-bs-target="#eikonHomeCarousel" data-bs-slide-to="7"></li>
        </ol>
        <div class="carousel-inner">
          <div class="carousel-item active">
            <img class="d-none d-md-block w-100" src="/images/slide/slide-living.webp" alt="First slide">
            <img class="d-block d-md-none w-100" src="/images/slide/slide-living-mobile.webp" alt="First slide">
          </div>
          <div class="carousel-item">
            <img class="d-none d-md-block w-100" src="/images/slide/bauhaus-all-afiche-bauhaus-slide.webp"
              alt="Second slide">
            <img class="d-block d-md-none w-100" src="/images/slide/bauhaus-all-afiche-bauhaus-slide-mobile.webp"
              alt="Second slide">
          </div>
          <div class="carousel-item">
            <img class="d-none d-md-block w-100" src="/images/slide/living_montreux.webp" alt="Third slide">
            <img class="d-block d-md-none w-100" src="/images/slide/living_montreux-mobile.webp" alt="Third slide">
          </div>
          <div class="carousel-item">
            <img class="d-none d-md-block w-100" src="/images/slide/slide-retail.webp" alt="Fourth slide">
            <img class="d-block d-md-none w-100" src="/images/slide/slide-retail-mobile.webp" alt="Fourth slide">
          </div>
          <div class="carousel-item">
            <img class="d-none d-md-block w-100" src="/images/slide/slide-living-double.webp" alt="Fifth slide">
            <img class="d-block d-md-none w-100" src="/images/slide/slide-living-double-mobile.webp" alt="Fifth slide">
          </div>
          <div class="carousel-item">
            <img class="d-none d-md-block w-100" src="/images/slide/modern.webp" alt="Sixth slide">
            <img class="d-block d-md-none w-100" src="/images/slide/modern-mobile.webp" alt="Sixth slide">
          </div>
          <div class="carousel-item">
            <img class="d-none d-md-block w-100" src="/images/slide/office.webp" alt="Seventh slide">
            <img class="d-block d-md-none w-100" src="/images/slide/office-mobile.webp" alt="Seventh slide">
          </div>
          <div class="carousel-item">
            <img class="d-none d-md-block w-100" src="/images/slide/animals.webp" alt="Eight slide">
            <img class="d-block d-md-none w-100" src="/images/slide/animals-mobile.webp" alt="Eight slide">
          </div>
        </div>
        <a class="carousel-control-prev" href="#eikonHomeCarousel" role="button" data-bs-slide="prev">
          <span class="carousel-control-prev-icon" aria-hidden="true"></span>
          <span class="sr-only">Previous</span>
        </a>
        <a class="carousel-control-next" href="#eikonHomeCarousel" role="button" data-bs-slide="next">
          <span class="carousel-control-next-icon" aria-hidden="true"></span>
          <span class="sr-only">Next</span>
        </a>
      </div>
      <section id="products">
        <div id="filter-button-reveal">
          <i class="fa-solid fa-sliders"></i>
        </div>
        <div class="container">
          <div class="row">
            <div id="filters-collapse" class="col-md-6 col-lg-3 collapse show">
              <div id="shop-page-filter-aside" class="sticky-top pb-5">
                <ul class="list-unstyled">
                  <li class="mb-3">
                    <div class="collapse show" id="type-collapse">
                      <div class="form-check form-switch">
                        <input class="form-check-input" type="checkbox" id="poster" onclick="check()">
                        <label class="form-check-label" for="poster">Afiches</label>
                      </div>
                      <div class="form-check form-switch">
                        <input class="form-check-input" type="checkbox" id="photo" onclick="check()">
                        <label class="form-check-label" for="photo">Fotografias</label>
                      </div>
                      <div class="form-check form-switch">
                        <input class="form-check-input" type="checkbox" id="postcard" onclick="check()">
                        <label class="form-check-label" for="postcard">Postales</label>
                      </div>
                    </div>
                  </li>
                  <li class="border-top my-3 w-50"></li>
                  <li class="mb-3">
                    <div class="collapse show" id="theme-collapse">
                      <div class="form-check form-switch">
                        <input class="form-check-input" type="checkbox" id="office" onclick="check()">
                        <label class="form-check-label" for="office">Aesthetics
                          offices</label>
                      </div>
                      <div class="form-check form-switch">
                        <input class="form-check-input" type="checkbox" id="home" onclick="check()">
                        <label class="form-check-label" for="home">Home Sweet Home</label>
                      </div>
                      <div class="form-check form-switch">
                        <input class="form-check-input" type="checkbox" id="professionals" onclick="check()">
                        <label class="form-check-label" for="professionals">Interioristas: Nuestra
                          selecci√≥n</label>
                      </div>
                      <div class="form-check form-switch">
                        <input class="form-check-input" type="checkbox" id="museum" onclick="check()">
                        <label class="form-check-label" for="museum">Originales y Series
                          Limitadas</label>
                      </div>
                    </div>
                  </li>
                  <li class="border-top my-3 w-50"></li>
                  <li class="mb-3">
                    <div class="collapse show" id="theme-collapse">
                      <div class="form-check form-switch">
                        <input class="form-check-input" type="checkbox" id="architecture" onclick="check()">
                        <label class="form-check-label" for="architecture">Architecture</label>
                      </div>
                      <div class="form-check form-switch">
                        <input class="form-check-input" type="checkbox" id="animals" onclick="check()">
                        <label class="form-check-label" for="animals">Animals</label>
                      </div>
                      <div class="form-check form-switch">
                        <input class="form-check-input" type="checkbox" id="vintage" onclick="check()">
                        <label class="form-check-label" for="vintage">Vintage</label>
                      </div>
                      <div class="form-check form-switch">
                        <input class="form-check-input" type="checkbox" id="bauhaus" onclick="check()">
                        <label class="form-check-label" for="bauhaus">Bauhaus</label>
                      </div>
                      <div class="form-check form-switch">
                        <input class="form-check-input" type="checkbox" id="maps" onclick="check()">
                        <label class="form-check-label" for="maps">Mapas</label>
                      </div>
                      <div class="form-check form-switch">
                        <input class="form-check-input" type="checkbox" id="blueprints" onclick="check()">
                        <label class="form-check-label" for="blueprints">Blueprints</label>
                      </div>
                    </div>
                  </li>
                </ul>
              </div>
            </div>
            <div class="col-md-6 col-lg-9">
              <div id="shop-page-items" class="row">
                <div id="no-results" class="col-12">No hay resultado usando esta combinacion de filtros
                </div>
                <% products.forEach(product=> { %>
                  <div
                    class="col-md-12 col-lg-6 mb-4 text-center product <%= product.category %> <%= product.space.join(' ') %> <%= product.theme.join(' ') %>">
                    <div id="<%= product.id %>" class="card">
                      <a href="/products/<%= product.id %>">
                        <img src="<%= product.coverImage %>" class="product-img" alt="<%= product.title %>">
                        <div class="product-info">
                          <span class="title">
                            <%= product.title %>
                          </span>
                          <span class="category">
                            <%= product.category %>
                          </span>
                          <span class="price">$ <%= product.price %></span>
                        </div>
                      </a>
                    </div>
                  </div>
                  <% }) %>
              </div>
            </div>
          </div>
        </div>
      </section>
      <!-- FOOTER -->
      <%- include('partials/footer') %>
        <!-- SCRIPTS -->
        <%- include('partials/scripts') %>
  </body>

</html>